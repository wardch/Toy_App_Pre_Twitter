<%= provide(:title, 'All Users') %>
<div class='row'>
  <aside class='col-md-r'>
      <table>
        <thead>
          <tr>
            <td>Name</td>
            <td>Email</td>
          </tr>
        </thead>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= link_to 'Show', user %></td>
            <td><%= link_to 'Edit', edit_user_path(user) %></td>
            <td><%= link_to 'Mutilate', user, method: :delete, data: {confirm: 'You sure dawg?'} %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
  </aside>
</div>
